---
import AstroRoot from "../components/AstroRoot.astro";
import { getWikiPaths, type WikiDTO } from "../lib/content";

export async function getStaticPaths() {
    return await getWikiPaths();
}

const { entry } = Astro.props;
const dto: WikiDTO = {
    id: entry.id,
    data: entry.data,
    body: entry.body,
}
---

<AstroRoot entry={dto} />
